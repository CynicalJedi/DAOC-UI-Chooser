    <WindowId>Clock</WindowId>
    <CloseButton>true</CloseButton>
    <MoveButton>true</MoveButton>
    <TopRightResizeButton>false</TopRightResizeButton>
    <BottomRightResizeButton>true</BottomRightResizeButton>
    <BottomLeftResizeButton>true</BottomLeftResizeButton>
    <ResizeButtonOffsetX>0</ResizeButtonOffsetX>
    <ResizeButtonOffsetY>0</ResizeButtonOffsetY>
    <TitleWidth>210</TitleWidth>
    <TitleHeight>5</TitleHeight>
    <Width>210</Width>
    <Height>25</Height>
    <ResizeableWidth>5</ResizeableWidth>
    <ResizeableHeight>5</ResizeableHeight>
    <ResizeableTwoWayWidth>0</ResizeableTwoWayWidth>
    <ResizeableTwoWayHeight>0</ResizeableTwoWayHeight>
    <MinWidth>210</MinWidth>
    <MinHeight>25</MinHeight>

    <!-- Background Image -->

    <FullResizeImageDef>
      <ControlId></ControlId>
      <Position>
        <X>0</X>
        <Y>0</Y>
      </Position>
      <Alignment>
        <GrowWidth>true</GrowWidth>
        <GrowHeight>true</GrowHeight>
      </Alignment>
      <TemplateName>fresizeimage_dlg_bg_small</TemplateName>
      <Width>210</Width>
      <Height>25</Height>
    </FullResizeImageDef>

    <!-- Local Time/Date-->

    <LabelDef>
      <ControlId></ControlId>
      <Position>
        <X>10</X>
        <Y>5</Y>
      </Position>
      <Alignment>        
      </Alignment>
      <Color>
        <R>255</R>
        <G>192</G>
        <B>0</B>
        <A>255</A>
      </Color>
      <FontName>font_buttons</FontName>
      <Width>120</Width>
      <Height>15</Height>
      <ColorAdapter></ColorAdapter>
      <MaxCharacters>20</MaxCharacters>
      <Data></Data>
      <EndAligned>false</EndAligned>
      <TextCentered>false</TextCentered>
      <Adapter>time_of_day</Adapter>
    </LabelDef>

    <!--Game Time-->

    <LabelDef>
      <ControlId></ControlId>
      <Position>
        <X>10</X>
        <Y>5</Y>
      </Position>
      <Alignment>
        <OffsetRight>true</OffsetRight>        
      </Alignment>
      <Color>
        <R>153</R>
        <G>204</G>
        <B>0</B>
        <A>255</A>
