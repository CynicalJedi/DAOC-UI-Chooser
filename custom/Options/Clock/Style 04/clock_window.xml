      <MaxCharacters>10</MaxCharacters>
      <Data></Data>
      <EndAligned>false</EndAligned>
      <TextCentered>false</TextCentered>
      <Adapter>game_time</Adapter>
    </LabelDef>

    <!-- Toggle Map window -->

    <InvisibleButtonDef>
      <ControlId></ControlId>
      <Position>
        <X>10</X>
        <Y>5</Y>
      </Position>
      <Alignment>
      </Alignment>
